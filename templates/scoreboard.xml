<fest:template xmlns:fest="http://fest.mail.ru" context_name="scores">
	<h1>Score Board</h1>
	<div class="scoreboard">
        <fest:each iterate="scores" index="i" value="score">
            <div><fest:value>score.login</fest:value>...
           <fest:value>score.score</fest:value></div>
        </fest:each>
    </div>
     <a href="#" class="gamepage__menu__infobar__menu-btn">Quit</a>
</fest:template>