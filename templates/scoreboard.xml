<fest:template xmlns:fest="http://fest.mail.ru">
	<h1>Score Board</h1>
	<div class="scoreboard">
        <fest:each iterate="scores" index="i" value="score">
            <div><fest:value>score.login</fest:value></div>
            <div><fest:value>score.value</fest:value></div>
        </fest:each>
    </div>
    <a href="#">Back</a>
</fest:template>